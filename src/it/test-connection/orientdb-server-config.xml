<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<orient-server>
    <network>
        <protocols>
            <protocol implementation="com.orientechnologies.orient.server.network.protocol.binary.ONetworkProtocolBinary" name="binary"/>
            <protocol implementation="com.orientechnologies.orient.server.network.protocol.http.ONetworkProtocolHttpDb" name="http"/>
        </protocols>
        <listeners>
            <listener protocol="binary" socket="default" port-range="2414-2420" ip-address="0.0.0.0"/>
            <listener protocol="http" socket="default" port-range="2470-2480" ip-address="0.0.0.0"/>
        </listeners>
    </network>
    <users>
        <user resources="*" password="root" name="hello"/>
    </users>
    <properties>
        <entry value="C:/work/dev/orientechnologies/orientdb/releases/1.0rc1-SNAPSHOT/www/" name="orientdb.www.path"/>
        <entry value="${basedir}/config/orientdb-server-config.xml" name="orientdb.config.file"/>
        <entry value="false" name="server.cache.staticResources"/>
        <entry value="info" name="log.console.level"/>
        <entry value="fine" name="log.file.level"/>
        <entry value="false" name="plugin.dynamic"/>
    </properties>
    <isAfterFirstTime>true</isAfterFirstTime>
</orient-server>
	